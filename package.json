    "client",
    "utils",
    "browser",
    "web",
    "front-end",
    "工具库",
    "函数库"
  ],
  "author": "xiaoqiujun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xiaoqiujun/funtool/issues"
  },
  "homepage": "https://github.com/xiaoqiujun/funtool#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "rollup": "^2.70.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
    "ts-node": "^10.6.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  }
